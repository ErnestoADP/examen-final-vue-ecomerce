{"ast":null,"code":"import \"core-js/modules/es.array.find.js\";\nimport \"core-js/modules/es.object.to-string.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport sagasData from \"../assets/api/database.json\";\nexport default {\n  name: \"Manga\",\n  data: function data() {\n    return {\n      manga: {}\n    };\n  },\n  mounted: function mounted() {\n    // obtener id desde la ruta\n    var id = this.$route.params.id; // poner coleccion de mangas en una variable \n\n    var coleccion = sagasData.sagas; // obtener solo un item de la coleccion por la id \n\n    this.manga = coleccion.find(function (item) {\n      return item.id == id;\n    });\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAmCA;AACA;AACAA,eADA;AAEAC,MAFA,kBAEA;AACA;AACAC;AADA;AAGA,GANA;AAOAC,SAPA,qBAOA;AAEA;AACA,mCAHA,CAKA;;AACA,oCANA,CAQA;;AACA;AAAA;AAAA;AACA;AAjBA","names":["name","data","manga","mounted"],"sourceRoot":"src/views","sources":["Manga.vue"],"sourcesContent":["<template>\n  <b-container class=\"bv-example-row mt-4 card-home\">\n    <b-row>\n      <b-col\n        ><div class=\"card-parent\">\n          <b-card  v-for=\"tomo in this.manga.tomos\"\n            :key=\"tomo.id\"\n            \n            :img-src=\"tomo.imagen\"\n            img-alt=\"Image\"\n            img-top\n            tag=\"article\"\n            style=\"max-width: 15rem\"\n            class=\"mb-2 card-interior\"\n          >\n          \n            <b-card-text>\n              <h3>Numero: {{ tomo.numero }}</h3>\n            </b-card-text>\n            <hr />\n            <b-card-text> <b>Precio:</b> {{ tomo.precio }} </b-card-text>\n            <hr />\n            <b-card-text> <b>Stock:</b> {{ tomo.stock }} </b-card-text>\n            <hr />\n\n            <b-button href=\"#\" variant=\"dark\">Agregar al Carrito</b-button>\n          </b-card>\n        </div></b-col\n      >\n    </b-row>\n  </b-container>\n  <!-- End Container -->\n</template>\n\n<script>\nimport sagasData from \"../assets/api/database.json\";\nexport default {\n  name: \"Manga\",\n  data() {\n    return {\n      manga: {}\n    };\n  },\n  mounted() {\n\n    // obtener id desde la ruta\n    let id = this.$route.params.id;\n\n    // poner coleccion de mangas en una variable \n    let coleccion = sagasData.sagas;\n\n    // obtener solo un item de la coleccion por la id \n    this.manga = coleccion.find((item) => item.id == id);\n  },\n};\n</script>\n\n<style></style>\n"]},"metadata":{},"sourceType":"module"}